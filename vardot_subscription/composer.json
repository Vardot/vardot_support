{
  "name": "vardot-proprietary/vardot_subscription",
  "description": "Vardot Support subscription widget.",
  "type": "drupal-custom-module",
  "homepage": "https://bitbucket.org/Vardot/vardot_subscription",
  "license": "proprietary",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Vardot",
      "homepage": "https://www.drupal.org/vardot",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://bitbucket.org/Vardot/vardot_subscription/issues",
    "source": "https://bitbucket.org/Vardot/vardot_subscription/src"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
      },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "composer/installers": "~1.0 || ~2.0",
    "oomphinc/composer-installers-extender": "~1.0 || ~2.0"
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-1.x": "8.1.x-dev"
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "enable-patching": true,
    "composer-exit-on-patch-failure": true
  }
}
